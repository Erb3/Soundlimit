{
  "schemaVersion": 1,
  "id": "soundlimit",
  "version": "${version}",
  "name": "soundlimit",
  "description": "A simple mod to increase the sound limit for Minecraft",
  "authors": ["Erb3"],
  "contact": {
    "repo": "https://github.com/Erb3/soundlimit"
  },
  "license": "GPL-3.0",
  "icon": "assets/soundlimit/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "github.erb3.mod.soundlimit.Soundlimit"
    ]
  },
  "mixins": [
    "soundlimit.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
